datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        String   @id @default(uuid())
  firstName String
  lastName  String
  email     String   @unique
  age       Int?
  password  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
